#include<stdio.h>  
#include <string.h>    

struct date1
{
int day;
int month;
int year;
};
struct date2
{
int day;
int month;
int year; 
};
void mian(){
int i=0;
int length=0;
int size=0;
print("enter size of databases : ");
scanf("%d",&size);
struct date1 std1[size];
struct date2 std2[size];
printf("\n Enter details of database 1 \n ");
for(i=0;i<size;i++)
{
printf("Enter dayNo");
scanf("%d",$std1[i].day);
printf("Enter Month ");
scanf("%d",$std1[i].month);
printf("Enter Year ");
scanf("%d",$std1[i].year);
}
printf("\n Enter details of 2nd databases :");
for(i=0;i<size;i++)
{
printf("Enter dayNo");
scanf("%d",$std1[i].day);
printf("Enter Month ");
scanf("%d",$std1[i].month);
printf("Enter Year ");
scanf("%d",$std1[i].year);
}
printf("Enter date of 1st data bases : ");
for(i=0;i<size;i++)
{
printf("\%d,:%d,:,%d",std1[i].day,std1[i],month,std1[i].yeear);    
}
printf("Enter date of 2nd data bases : ");
for(i=0;i<size;i++){
printf("\%d,:%d,:,%d",std2[i].day,std2[i],month,std2[i].yeear);    
}
// compareong both databases : 
for(i=0;i<size;i++)
{
if(std1[i].day==std2[i].day)
{
	if(std2[i].month==std2[i].month)
	{
	if(std1[i].year==std2[i].year)
	{
	length++;
	}
	else
	{
	break;
	}
	}
	else
	{
	break;
	}
}
else
{
break;
}
}
if(length==size)
{
printf(" Both DataBases are equals :" ):
}
else
{
printf(" Both database are not equals :");
return 0;
}
