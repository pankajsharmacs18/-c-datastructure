#include<stdio.h>
#include<String.h>
struct Student
{
char rollNo[10];
int age;
char name[10];
};
void main()
{
int i=0;
int size;
printf("Enter no of student : ");
scanf("%d",&size);
struct Student st[size];
for(i=0;i<size;i++)
{
printf("enter name : ");
scanf("%s",&st[i].name);
printf("enter age  : ");
scanf("%d",&st[i].age);
printf("enter roll : ");
scanf("%s",&st[i].rollNo);
}
printf("name :  age ::  roll NO :");
for(i=0;i<size;i++)
{
	printf("\n %s :,  %d  : :, %d :",st[i].name,st[i].age,st[i].rollNo);
}
return;
}

